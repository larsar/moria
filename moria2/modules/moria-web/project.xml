<?xml version="1.0" encoding="UTF-8"?>

<!-- $Id$ -->

<project>
  <extend>${basedir}/../../project.xml</extend>
  <name>Moria Web Frontends</name>
  <id>moria-web</id>
  <currentVersion>1.0-b1-SNAPSHOT</currentVersion>
  <package>no.feide.moria.servlet</package>

  <shortDescription>Moria web frontends (HTML and SOAP)</shortDescription>

  <description/>

  <siteDirectory>/home/groups/m/mo/moria/htdocs/moria-web</siteDirectory>

  <dependencies>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.4-20030804.000000</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>xerces</groupId>
      <artifactId>xercesImpl</artifactId>
      <version>2.6.2</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>axis</groupId>
      <artifactId>axis</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>axis</groupId>
      <artifactId>axis-ant</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>jaxrpc</groupId>
      <artifactId>jaxrpc</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>saaj</groupId>
      <artifactId>saaj</artifactId>
      <version>1.4</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>wsdl4j</groupId>
      <artifactId>wsdl4j</artifactId>
      <version>1.4</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0b10</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.3</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>commons-discovery</groupId>
      <artifactId>commons-discovery</artifactId>
      <version>0.2</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>moria</groupId>
      <artifactId>moria-am</artifactId>
      <version>1.0-b1-SNAPSHOT</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <!--
    <dependency>
    <groupId>moria</groupId>
    <artifactId>moria-dm</artifactId>
    <version>1.0-b1-SNAPSHOT</version>
    <type>jar</type>
    <properties>
    <war.bundle>true</war.bundle>
    </properties>
    </dependency>
    -->
    <dependency>
      <groupId>moria</groupId>
      <artifactId>moria-ctrl</artifactId>
      <version>1.0-b1-SNAPSHOT</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

    <dependency>
      <groupId>moria</groupId>
      <artifactId>moria-log</artifactId>
      <version>1.0-b1-SNAPSHOT</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
  </dependencies>

  <build>
    <resources>
      <resource>
        <directory>src/conf</directory>
        <includes>
          <include>*.xml</include>
          <include>*.properties</include>
        </includes>
      </resource>
    </resources>
    <unitTest>
      <resources>
        <resource>
          <directory>src/test/bundles</directory>
          <includes>
            <include>*.properties</include>
          </includes>
        </resource>
        <resource>
          <directory>src/test/conf</directory>
          <includes>
            <include>*.properties</include>
          </includes>
        </resource>
      </resources>
    </unitTest>
  </build>
</project>
