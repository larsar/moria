<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

    <servlet>
        <servlet-name>Moria1Demo</servlet-name>
        <servlet-class>no.feide.mellon.demo.Moria1DemoServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Moria2Demo</servlet-name>
        <servlet-class>no.feide.mellon.demo.Moria2DemoServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Moria1Demo</servlet-name>
        <url-pattern>/Moria1Demo</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Moria2Demo</servlet-name>
        <url-pattern>/Moria2Demo</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Moria2Demo</servlet-name>
        <url-pattern>/Demo</url-pattern>
    </servlet-mapping>

</web-app>